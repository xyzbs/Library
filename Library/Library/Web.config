<configuration>
  <connectionStrings>
    <add name="LibraryConnectionString" connectionString="Data Source=DESKTOP-DFOPNE4;Initial Catalog=Library;Integrated Security=True"
      providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.webServer>
    <security>
      <requestFiltering allowDoubleEscaping="true">
        <fileExtensions allowUnlisted="true">
          <remove fileExtension=".ascx"/>
